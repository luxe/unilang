◀private: static▶ std::string ☀Get_Body(model::Module const& structure, model::Function const& method, Java_Settings const& settings)❰
    std::string x;
    
    bool previous_was_comment = false;
    for (auto const& statement: First_Element_Or_Default(method.body).statement){
            auto fk = First_Element_Or_Default(statement.name);
            auto pk = Resolve(structure,fk);
            
            //space out comment blocks
            if (Is_Comment(structure,pk,settings)){
                
                if (!previous_was_comment){
                    x += "\n\n";
                    x += Get_Body_Statement(structure,pk,settings);
                }
                else{
                    
                    //This is so code formats better.
                    x += Coninue_Existing_Comment(structure,pk,settings);
                }
            }
            else {
                if (previous_was_comment){
                    x += "\n";
                }
                x += Coninue_Existing_Comment(structure,pk,settings);
            }
            
            
            
            previous_was_comment = Is_Comment(structure,pk,settings);
            
            
    }
    return x;
❱



◀private: static▶ std::string ☀Get_Body_Statement(model::Module const& structure, model_variants::FunctionBodyStatementName const& any_statement, Java_Settings const& settings)❰
    std::string str;
    
        std::visit(overloaded {
            [&](model::RawStatement const& statement) {
                str += First_Element_Or_Default(statement.text).value;
                str += "\n";
            },
            [&](model::TodoStatement const& statement) {
                str += "// TODO(thickey): " + First_Element_Or_Default(statement.text).value;
                //str += "\n";
            },
            [&](model::StatementsComment const& statement) {
                str += "// " + First_Element_Or_Default(statement.text).value;
                //str += "\n";
            },
        }, any_statement);
    
    return str;
❱

◀private: static▶ std::string ☀Coninue_Existing_Comment(model::Module const& structure, model_variants::FunctionBodyStatementName const& any_statement, Java_Settings const& settings)❰
    std::string str;
    
        std::visit(overloaded {
            [&](model::RawStatement const& statement) {
                str += First_Element_Or_Default(statement.text).value;
            },
            [&](model::TodoStatement const& statement) {
                str += " TODO(thickey): " + First_Element_Or_Default(statement.text).value;
            },
            [&](model::StatementsComment const& statement) {
                str += " " + First_Element_Or_Default(statement.text).value;
            },
        }, any_statement);
    
    return str;
