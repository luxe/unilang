◀public: static▶ std::string ☀Get(model::Module const& structure, std::vector<Contributor> const& contributors, Java_Settings const& settings)❰
    std::string x;
    for (auto it: First_Element_Or_Default(structure.functions).function){
         x += Doxygen_Block_Printer::Get_Indented_Block(structure, it,First_Element_Or_Default(structure.functions).function,settings.module,4);
         x += "\n";
         x += Get_Signature(structure,it,settings);
         x += "\n";
         x += Get_Possible_Body(structure,it,settings);
    }
    
    return x;
