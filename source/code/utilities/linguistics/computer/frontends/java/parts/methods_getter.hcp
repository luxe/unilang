◀private: static▶ std::string ☀Possible_Thrown_Exceptions(model::Function const& method, Java_Settings const& settings)❰
    std::string x;
    
    //extract exception types
    std::vector<std::string> except_types;
    for (auto const& it: method.except){
        auto type = First_Element_Or_Default(it.statement).value;
        except_types.emplace_back(type);
    }
    
    //possibly print them
    if (!except_types.empty()){
        x+= " throws " + Get_As_Comma_Seperated_String(except_types);
    }
    
    return x;
