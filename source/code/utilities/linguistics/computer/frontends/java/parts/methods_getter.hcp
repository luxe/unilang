◀private: static▶ bool ☀Is_Comment(model::Module const& structure, model_variants::FunctionBodyStatementName const& any_statement, Java_Settings const& settings)❰
    bool is_comment = false;
    
        std::visit(overloaded {
            [&](model::RawStatement const& statement) {
                is_comment = false;
            },
            [&](model::TodoStatement const& statement) {
                is_comment = true;
            },
            [&](model::StatementsComment const& statement) {
                is_comment = true;
            },
        }, any_statement);
        
    return is_comment;
