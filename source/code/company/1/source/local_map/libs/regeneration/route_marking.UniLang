    if (􏵲is new route􏵳route)){
        return true;
    }
    if (􏵲is expired route􏵳route)){
        return true;
    }
    if (􏵲is invalid route􏵳route)){
        return true;
    }
    if (􏵲is existing route􏵳route)){
        return *route.evaluation_range.previous <= route.evaluation_range.current;
