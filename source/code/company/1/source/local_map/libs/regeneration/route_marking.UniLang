    std::map<hub::base::guid,size_t> seen;
    stdx::optional<hub::base::guid> previous;
    
    
    􏵲iterate with algorithm window􏵳route,range,[&](size_t i){
        auto id = local_map::internal::submap_identifier(route.lane_sequence[i]);
        
