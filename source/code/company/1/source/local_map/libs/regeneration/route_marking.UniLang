􏶴
    // gather up all of the broken invariants
    // Although these broken invariants are handled safely in the regeneration framework,
    // they result in questionable awareness of SLMT's behavior
    auto checked_invariants = 􏵲check invariants􏵳route);
    if (!checked_invariants.evaluation_range_consistent){
        messages.emplace_back("The evaluation range is inconsistent. You're telling me you went backwards.");
    }
    if (!checked_invariants.evaluation_range_in_bounds){
        messages.emplace_back("The evaluation range is out of bounds from the actual route.");
    }
    if (!checked_invariants.has_not_jumped_over_1_or_more_lanes){
        messages.emplace_back("You jumped over a lane.");
    }
    if (!checked_invariants.has_route_hysteresis){
        messages.emplace_back("You don't have any route hysteresis.");
    }
    if (!checked_invariants.exists_on_or_after_merge_point){
        messages.emplace_back("You claim to be on a route before the merge point.");
    }
    if (!checked_invariants.merge_point_in_bounds){
        messages.emplace_back("The merge point exists outside the route.");
    }
    if (!checked_invariants.route_has_lanes){
        messages.emplace_back("The route doesn't have any lanes.");
    }
    if (!checked_invariants.all_lanes_have_size){
        messages.emplace_back("Some of the route lanes are of distance 0.");
    }
