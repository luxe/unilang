◀private: static▶ Spawn_Pairings ☀Run_AB_Test_Sequentially(Settings const& settings) ❰

    Maybe_Output_And_Flush_With_Newline(settings.verbose,"(Test A) building target...");
    auto bazel_1 = Prepend_Cd_Path(settings.bazel_wrapper.program_name,settings.a_path);
    Possibly_Augment_System(settings);
    auto spawns1 = Bazel_Runner::Get_Spawn_Information_As_Cmd_Map(bazel_1,settings.a_environment,settings.target_name);
    
    Maybe_Output_And_Flush_With_Newline(settings.verbose,"(Test B) building target...");
    auto bazel_2 = Prepend_Cd_Path(settings.bazel_wrapper.program_name,settings.b_path);
    Possibly_Augment_System(settings);
    auto spawns2 = Bazel_Runner::Get_Spawn_Information_As_Cmd_Map(bazel_2,settings.b_environment,settings.target_name);
    
    Maybe_Output_And_Flush_With_Newline(settings.verbose,"Pairing together subcommands...");
    auto pairings = Spawn_Pairer::Build_Matches_And_Unpairable_Spawns(spawns1,spawns2);
    
    return pairings;
