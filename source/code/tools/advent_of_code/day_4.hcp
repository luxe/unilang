◀private: static▶ bool ☀Valid_Pass(int num) ❰
    auto str = std::to_string(num);
    std::cout << str << std::endl;
    
    int at_num = -1;
    int consec_count = 1;
    
    bool c_yes = false;
    
    for (auto it: str){
        std::string x(1,it);
        auto curr_num = as_signed(x);
        
        
        if (curr_num == at_num){
            consec_count++;
        }
        else{
            if (consec_count == 2){
                c_yes = true;
            }
            consec_count = 1;
        }
        
        
        
        //consecutive check
        if (curr_num < at_num){
            return false;
        }
        at_num = curr_num;
    }
    
    if (consec_count == 2){
        c_yes = true;
    }
    
    std::cout << "here" << std::endl;
    return c_yes;
