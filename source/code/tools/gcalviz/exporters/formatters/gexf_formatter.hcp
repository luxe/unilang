◀private: static▶ void ☀Set_Employee_Extraction_Step(Gexf_Attribute& attr, std::string name)❰
    Json_Extraction_Step step;
    step.key = "employee";
    step.index = false;
    attr.extract.desired_key = name;
    attr.extract.steps.clear();
    attr.extract.steps.emplace_back(step);
